{
  "name": "ati-broadcastapp",
  "version": "1.0.0",
  "description": "Allow AITD trusted partners to broadcast Facebook messages to multiple Facebook groups in their domain.",
  "main": "index.js",
  "scripts": {
    "start-server": "nodemon -w server dist/server.js",
    "start": "npm run start-server",
    "compile": "webpack",
    "compile-server": "babel server --out-dir dist --source-maps",
    "compile-local-tests": "babel tests-local --out-dir tests-local-compiled --source-maps",
    "watch": "webpack-dev-server --hot --inline",
    "watch-server": "babel server --out-dir dist --source-maps --watch",
    "watch-local-tests": "babel tests-local --out-dir tests-local-compiled --source-maps --watch",
    "dev-all": "npm run watch-server & npm run watch & npm start-server",
    "test": "npm run lint",
    "lint": "eslint --ext js,jsx src server webpack.config.js",
    "postinstall": "ln -sf ../node_modules/bootstrap/dist/ static/bootstrap",
    "prestart": "npm run compile & npm run compile-server"
  },
  "engines": {
    "node": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Data4Democracy/ati-broadcastapp.git"
  },
  "keywords": [
    "indivisible",
    "data4democracy"
  ],
  "author": "Data4Democracy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Data4Democracy/ati-broadcastapp/issues"
  },
  "homepage": "https://github.com/Data4Democracy/ati-broadcastapp#readme",
  "dependencies": {
    "@google-cloud/debug-agent": "^2.1.3",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.2",
    "bootstrap": "^3.3.7",
    "express": "^4.15.4",
    "moment": "^2.18.1",
    "mongoose": "^4.11.7",
    "mz": "^2.6.0",
    "nconf": "^0.8.4",
    "nodemon": "^1.11.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-router-bootstrap": "^0.24.2",
    "react-router-dom": "^4.1.2",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "source-map-support": "^0.4.16",
    "webpack": "^3.5.5",
    "whatwg-fetch": "^2.0.3",
    "react-google-login": "^2.9.3"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-react": "^7.2.1",
    "webpack-dev-server": "^2.7.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "sourceMaps": true
  }
}
